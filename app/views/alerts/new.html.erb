<h1><%=t '.new_alert' %></h1>
<div class="default-form">
  <fieldset>
  <legend><%=t '.legend' %></legend>
  <%= form_for(@alert) do |f| %>

    <label><%=t '.host' %>:</label>
    <%= f.text_field :host_id %><br />

    <label><%=t '.service' %>:</label>
    <%= f.text_field :service_id %><br />

    <label><%=t '.date_start' %>:</label>
    <%= f.text_field :date_start %><br />

    <label><%=t '.date_end' %>:</label>
    <%= f.text_field :date_end %><br />

    <label><%=t '.check_status' %>:</label>
    <%= f.text_field :check_status %><br />

    <label><%=t '.service_output' %>:</label>
    <%= f.text_field :service_output %><br />

    <label><%=t '.duration' %>:</label>
    <%= f.text_field :duration %><br />

    <label><%=t '.report_id' %>:</label>
    <%= f.text_field :report_id %><br />

    <%= f.submit t('generic.create') %>
  <% end %>
  </fieldset>
</div>

<%= link_to t('generic.back'), alerts_path %>
